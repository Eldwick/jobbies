<div id="newCompanyWrapper">
<h2>Create a company from linkedin</h2>
<%= form_tag '/company/create_from_linkedin' do %>
  <%= text_field_tag "companyname", nil, placeholder: "Company Name" %><br><br>
  <%= submit_tag "Create Company from LinkedIn", class: "btn btn-success"  %>
<% end %>
<h2>New company</h2>

<%= render 'form' %>

<%= link_to 'Back', companies_path %>
</div>
